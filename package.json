{
  "name": "kolm-expo",
  "description": "A very minimal Expo starter with API routes, tRPC, Drizzle ORM, better-auth and TailwindCSS",
  "author": "Jelle Kuipers <info@jellekuipers.com> (https://jellekuipers.com)",
  "main": "expo-router/entry",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:reset": "npx tsx src/db/reset.ts ",
    "db:seed": "npx tsx src/db/seed.ts ",
    "db:seed:dev": "npx tsx src/db/seed.dev.ts ",
    "db:studio": "drizzle-kit studio",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "npx @biomejs/biome lint --write",
    "format": "npx @biomejs/biome format --write",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "@better-auth/expo": "^1.3.6",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@tanstack/react-query": "^5.85.2",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@trpc/tanstack-react-query": "^11.4.4",
    "better-auth": "^1.3.6",
    "drizzle-orm": "^0.44.4",
    "drizzle-seed": "^0.3.1",
    "expo": "53.0.20",
    "expo-blur": "14.1.5",
    "expo-constants": "17.1.7",
    "expo-font": "13.3.2",
    "expo-haptics": "14.1.4",
    "expo-linking": "7.1.7",
    "expo-router": "5.1.4",
    "expo-secure-store": "14.2.3",
    "expo-splash-screen": "0.30.10",
    "expo-status-bar": "2.2.3",
    "expo-symbols": "0.4.5",
    "expo-system-ui": "5.0.10",
    "expo-web-browser": "14.2.0",
    "nativewind": "4.1.23",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "2.24.0",
